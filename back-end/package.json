{
  "name": "VBA Project",
  "version": "1.0.0",
  "description": "Node.js Rest Apis with Express, Sequelize & MySQL.",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate": "npx sequelize-cli db:migrate ",
    "rollback": "npx sequelize-cli db:migrate:undo",
    "rollback:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "rollback:seed": "npx sequelize-cli db:seed:undo",
    "rollback:seed:all": "npx sequelize-cli db:seed:undo:all",
    "prepare": "husky install && npx husky add .husky/pre-commit \"npx lint-staged\"",
    "format:write": "prettier \"**/*.{js,ts}\" --write",
    "format:check": "prettier \"**/*.{js,ts}\" --check",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "mysql",
    "rest",
    "api"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": "prettier --write"
  },
  "author": "Alvin",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "joi": "^17.12.3",
    "jsonwebtoken": "^9.0.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.4",
    "nodemailer": "^6.9.7",
    "sequelize": "^6.35.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.6.2"
  }
}
